extends _layout

block content
	h1 Accounts
		if editable
			| <a href="/admin/users/new" class="btn btn-default">Add New</a>
	p.lead Accounts give more people access to manage your website.

	- var errors = req.flash('error');
	- var successes = req.flash('success');

	each error in errors
		.alert.alert-danger
			p= error
	each success in successes
		.alert.alert-success
			p= success

	+table(table, !editable)
